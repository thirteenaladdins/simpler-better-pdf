		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.8.0",
		"rollup-plugin-copy": "^3.5.0",
		"svelte": "^5.22.4",
		"svelte-check": "^3.4.3",
		"svelte-feather-icons": "^4.2.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.0",
		"vitest": "^3.0.7"
	},
	"type": "module",
	"dependencies": {
		"@nutrient-sdk/viewer": "^1.2.0",
		"axios": "^1.5.0",
